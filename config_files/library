server {
   listen 80;
    server_name 172.16.100.205;
    location = /favicon.ico { access_log off; log_not_found off; }
    location /staticfiles/ {
        root /home/user/projects/library;
    }
    location / {
        include proxy_params;
        proxy_pass http://unix:/home/user/projects/library/library.sock;
    }
}

